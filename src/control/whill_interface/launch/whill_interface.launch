<?xml version="1.0"?>
<launch>
  <arg name="twist_topic" default="/cmd_vel" />
  <arg name="joy_topic" default="/whill/controller/joy" />

  <node name="whill_interface_node" pkg="whill_interface" type="whill_interface_node" output="screen" respawn="true" >
    <param name="twist_topic" value="$(arg twist_topic)" />
    <param name="joy_topic" value="$(arg joy_topic)" />
  </node>
</launch>
